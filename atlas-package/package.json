{
  "name": "@quietloudlab/ai-interaction-atlas",
  "version": "1.0.4",
  "description": "A shared language for designing AI experiences - 100+ interaction patterns across human actions, AI tasks, system operations, data, constraints, and touchpoints",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "import": "./dist/types.mjs",
      "require": "./dist/types.js"
    },
    "./data": {
      "types": "./dist/data.d.ts",
      "import": "./dist/data.mjs",
      "require": "./dist/data.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "ai",
    "interaction-design",
    "taxonomy",
    "patterns",
    "llm",
    "ai-ux",
    "design-system",
    "workflow",
    "ai-design",
    "human-ai-interaction",
    "ai-product",
    "ai-systems",
    "atlas"
  ],
  "author": "Brandon Harwood <brandon@quietloudlab.com>",
  "homepage": "https://ai-interaction.com",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/quietloudlab/ai-interaction-atlas.git",
    "directory": "atlas-package"
  },
  "bugs": {
    "url": "https://github.com/quietloudlab/ai-interaction-atlas/issues"
  },
  "license": "Apache-2.0",
  "sideEffects": false,
  "devDependencies": {
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
